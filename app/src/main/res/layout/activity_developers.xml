            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="10dp"
            android:id="@+id/image4"
            android:src="@drawable/aman"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
