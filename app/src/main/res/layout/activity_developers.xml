<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".developers">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_margin="10dp"
            android:id="@+id/image1"
            android:src="@drawable/shridhar"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="SHRIDHAR GOEL(Mentor)"
            android:layout_marginLeft="10dp"
            android:textSize="20sp"
            android:textColor="@android:color/darker_gray"
            android:layout_toRightOf="@id/image1"
            android:id="@+id/name1"/>
        <TextView
            android:id="@+id/branch1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mathematics and Computing"
            android:layout_marginLeft="10dp"
            android:textSize="15sp"
            android:textColor="@android:color/darker_gray"
            android:layout_toRightOf="@id/image1"
            android:layout_below="@id/name1"/>
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@id/branch1"
            android:layout_toRightOf="@id/image1"
            android:onClick="shridhar"
            android:src="@drawable/git"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100dp">
        <ImageView
